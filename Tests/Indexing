//THE CODE

effect {
    Name: "ToPaMi",
    Action: (targets, context) => {
        iter = 0;
        otherField = context.OtherField;
        while(iter++<otherField.Count()-1){
            otherField.Pop();
        };
    }
}
card {
    Type: "Oro",
    Name: "Loraimis",
    Faction: "Rick"@@"Sanchez",
    Power: 1,
    Range: ["Melee"],//40
    OnActivation: [
        {
            Effect: "ToPaMi"
        }
    ]
}